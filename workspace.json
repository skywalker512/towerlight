{
  "version": 2,
  "projects": {
    "forum": {
      "root": "apps/forum",
      "sourceRoot": "apps/forum",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "root": "apps/forum",
            "outputPath": "dist/apps/forum"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "forum:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "forum:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "forum:build:production"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/forum/**/*.{ts,tsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/forum"
          ],
          "options": {
            "jestConfig": "apps/forum/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "forum-e2e": {
      "root": "apps/forum-e2e",
      "sourceRoot": "apps/forum-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/forum-e2e/cypress.json",
            "tsConfig": "apps/forum-e2e/tsconfig.e2e.json",
            "devServerTarget": "forum:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "forum:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/forum-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "forum"
}